@using Microsoft.AspNetCore.Mvc.TagHelpers;
@{
    ViewData["Title"] = "Home Page";
    var local = ViewBag.Local != null ? (List<Local>)ViewBag.Local : null;
}

@model List<Agendamento>

<div class="row flex-column">
    <div>
        <partial name="~/Views/Local/Index.cshtml" model="local"/>
    </div>
    <div style="margin-top: 50px;">
        <partial name="~/Views/Agendamento/Index.cshtml" model="Model" />
    </div>
</div>